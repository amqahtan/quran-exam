        <script>
            function computeResult() {

                var level = document.getElementById('level').value;
                var exam_type = document.getElementById('exam_type').value;
                var no_of_questions;
                var max_value;
                var zero_value;
                var tajweed_final;
                var telawa;
                var hefth;
                var Total_Mark;
				var Grade;
				


                var tajweed_deduction_half_point = document.getElementById('tajweed_deduction_half_point').value;
                var tajweed_deduction_1_point = document.getElementById('tajweed_deduction_1_point').value;
                var daqaeq_deduction = document.getElementById('daqaeq_deduction').value;
                var salasa_deduction = document.getElementById('salasa_deduction').value;
                var tazyeen_deduction = document.getElementById('tazyeen_deduction').value;
                var waqf_deduction_half_point = document.getElementById('waqf_deduction_half_point').value;
                var waqf_deduction_1_point = document.getElementById('waqf_deduction_1_point').value;


                var daqaeq_final = (20 - (daqaeq_deduction));
                var salasa_final = (10 - (salasa_deduction));
                var tazyeen_final = (5 - (tazyeen_deduction));
                var waqf_total_half_point = (waqf_deduction_half_point * 0.5);
                var waqf_total_1_poin = (waqf_deduction_1_point * 1);
                var waqf_final = (5 - (waqf_total_half_point + waqf_total_1_poin));


                var ethterab_deduction_Q1 = document.getElementById('ethterab_deduction_Q1').value;
                var tanbeeh_deduction_Q1 = document.getElementById('tanbeeh_deduction_Q1').value;
                var fath_deduction_Q1 = document.getElementById('fath_deduction_Q1').value;
                var ethterab_deduction_Q2 = document.getElementById('ethterab_deduction_Q2').value;
                var tanbeeh_deduction_Q2 = document.getElementById('tanbeeh_deduction_Q2').value;
                var fath_deduction_Q2 = document.getElementById('fath_deduction_Q2').value;
                var ethterab_deduction_Q3 = document.getElementById('ethterab_deduction_Q3').value;
                var tanbeeh_deduction_Q3 = document.getElementById('tanbeeh_deduction_Q3').value;
                var fath_deduction_Q3 = document.getElementById('fath_deduction_Q3').value;
                var ethterab_deduction_Q4 = document.getElementById('ethterab_deduction_Q4').value;
                var tanbeeh_deduction_Q4 = document.getElementById('tanbeeh_deduction_Q4').value;
                var fath_deduction_Q4 = document.getElementById('fath_deduction_Q4').value;
                var ethterab_deduction_Q5 = document.getElementById('ethterab_deduction_Q5').value;
                var tanbeeh_deduction_Q5 = document.getElementById('tanbeeh_deduction_Q5').value;
                var fath_deduction_Q5 = document.getElementById('fath_deduction_Q5').value;
                var ethterab_deduction_Q6 = document.getElementById('ethterab_deduction_Q6').value;
                var tanbeeh_deduction_Q6 = document.getElementById('tanbeeh_deduction_Q6').value;
                var fath_deduction_Q6 = document.getElementById('fath_deduction_Q6').value;
                var ethterab_deduction_Q7 = document.getElementById('ethterab_deduction_Q7').value;
                var tanbeeh_deduction_Q7 = document.getElementById('tanbeeh_deduction_Q7').value;
                var fath_deduction_Q7 = document.getElementById('fath_deduction_Q7').value;
                var ethterab_deduction_Q8 = document.getElementById('ethterab_deduction_Q8').value;
                var tanbeeh_deduction_Q8 = document.getElementById('tanbeeh_deduction_Q8').value;
                var fath_deduction_Q8 = document.getElementById('fath_deduction_Q8').value;
                var ethterab_deduction_Q9 = document.getElementById('ethterab_deduction_Q9').value;
                var tanbeeh_deduction_Q9 = document.getElementById('tanbeeh_deduction_Q9').value;
                var fath_deduction_Q9 = document.getElementById('fath_deduction_Q9').value;
                var ethterab_deduction_Q10 = document.getElementById('ethterab_deduction_Q10').value;
                var tanbeeh_deduction_Q10 = document.getElementById('tanbeeh_deduction_Q10').value;
                var fath_deduction_Q10 = document.getElementById('fath_deduction_Q10').value;

                var hefth_q1 = (20 - ((ethterab_deduction_Q1 * 0.25) + (tanbeeh_deduction_Q1 * 0.5) + (fath_deduction_Q1 * 2)));
                var hefth_q2 = (20 - ((ethterab_deduction_Q2 * 0.25) + (tanbeeh_deduction_Q2 * 0.5) + (fath_deduction_Q2 * 2)));
                var hefth_q3 = (20 - ((ethterab_deduction_Q3 * 0.25) + (tanbeeh_deduction_Q3 * 0.5) + (fath_deduction_Q3 * 2)));
                var hefth_q4 = (20 - ((ethterab_deduction_Q4 * 0.25) + (tanbeeh_deduction_Q4 * 0.5) + (fath_deduction_Q4 * 2)));
                var hefth_q5 = (20 - ((ethterab_deduction_Q5 * 0.25) + (tanbeeh_deduction_Q5 * 0.5) + (fath_deduction_Q5 * 2)));
                var hefth_q6 = (20 - ((ethterab_deduction_Q6 * 0.25) + (tanbeeh_deduction_Q6 * 0.5) + (fath_deduction_Q6 * 2)));
                var hefth_q7 = (20 - ((ethterab_deduction_Q7 * 0.25) + (tanbeeh_deduction_Q7 * 0.5) + (fath_deduction_Q7 * 2)));
                var hefth_q8 = (20 - ((ethterab_deduction_Q8 * 0.25) + (tanbeeh_deduction_Q8 * 0.5) + (fath_deduction_Q8 * 2)));
                var hefth_q9 = (20 - ((ethterab_deduction_Q9 * 0.25) + (tanbeeh_deduction_Q9 * 0.5) + (fath_deduction_Q9 * 2)));
                var hefth_q10 = (20 - ((ethterab_deduction_Q10 * 0.25) + (tanbeeh_deduction_Q10 * 0.5) + (fath_deduction_Q10 * 2)));
                //----------------------------------------------------------------------------------------------------------------------------------


                //----------------------------------------------------------------------------------------------------------------------------------
                if (level == '1') {
                    (document.getElementById('max_value').value = 70) && (document.getElementById('zero_value').value = 35);
                    max_value = 70;
                    zero_value = 35
                } else if (level == '2') {
                    (document.getElementById('max_value').value = 75) && (document.getElementById('zero_value').value = 38);
                    max_value = 75;
                    zero_value = 38;
                } else if (level == '3') {
                    (document.getElementById('max_value').value = 80) && (document.getElementById('zero_value').value = 40);
                    max_value = 80;
                    zero_value = 40;
                } else if (level == '4') {
                    (document.getElementById('max_value').value = 85) && (document.getElementById('zero_value').value = 43);
                    max_value = 85;
                    zero_value = 43;
                } else if (level == '5') {
                    (document.getElementById('max_value').value = 90) && (document.getElementById('zero_value').value = 45);
                    max_value = 90;
                    zero_value = 45;
                } else {
                    (document.getElementById('max_value').value = 100) && (document.getElementById('zero_value').value = 50);
                    max_value = 100;
                    zero_value = 50
                }
                //----------------------------------------------------------------------------------------------------------------------------------



                //----------------------------------------------------------------------------------------------------------------------------------

                if (exam_type == '1') {
                    document.getElementById('no_of_questions').value = "4";
                    no_of_questions = 4;
                } else if (exam_type == '2') {
                    document.getElementById('no_of_questions').value = "5";
                    no_of_questions = 5;
                } else if (exam_type == '3' || exam_type == '4') {
                    document.getElementById('no_of_questions').value = "10";
                    no_of_questions = 10;
                } else {
                    document.getElementById('no_of_questions').value = "";
                    no_of_questions = "";
                }
                //----------------------------------------------------------------------------------------------------------------------------------



                //----------------------------------------------------------------------------------------------------------------------------------
                if (no_of_questions == '4') {
                    document.getElementById('tajweed_final').value = (60 - ((tajweed_deduction_half_point * 0.5) + (tajweed_deduction_1_point * 1)));
                    tajweed_final = (60 - ((tajweed_deduction_half_point * 0.5) + (tajweed_deduction_1_point * 1)));
                } else if (no_of_questions == '5') {
                    document.getElementById('tajweed_final').value = (60 - (((tajweed_deduction_half_point * 0.5) + (tajweed_deduction_1_point * 1)) / 2));
                    tajweed_final = (60 - (((tajweed_deduction_half_point * 0.5) + (tajweed_deduction_1_point * 1)) / 2));
                } else if (no_of_questions == '10') {
                    (60 - (((tajweed_deduction_half_point * 0.5) + (tajweed_deduction_1_point * 1)) / 4));
                    document.getElementById('tajweed_final').value = tajweed_final = (60 - (((tajweed_deduction_half_point * 0.5) + (tajweed_deduction_1_point * 1)) / 4));
                } else {
                    document.getElementById('tajweed_final').value = "";
                    tajweed_final = "";
                }
                //----------------------------------------------------------------------------------------------------------------------------------



                //----------------------------------------------------------------------------------------------------------------------------------
                if (((tajweed_final) + (daqaeq_final) + (salasa_final) + (tazyeen_final) + (waqf_final)) > max_value) {
                    document.getElementById('tajweed_final').value = 100;
                    telawa = 100;
                } else {
                    document.getElementById('tajweed_final').value = ((((tajweed_final)+(daqaeq_final)+(salasa_final)+(tazyeen_final)+(waqf_final))/ max_value) * 100);
                    telawa = ((((tajweed_final)+(daqaeq_final)+(salasa_final)+(tazyeen_final)+(waqf_final))/ max_value) * 100);

                }
                //----------------------------------------------------------------------------------------------------------------------------------


                //----------------------------------------------------------------------------------------------------------------------------------
                if (no_of_questions == '4') {
                    hefth = ((hefth_q1 + hefth_q2 + hefth_q3 + hefth_q4) * 1.25);
                } else if (no_of_questions == '10') {
                    hefth = ((hefth_q1 + hefth_q2 + hefth_q3 + hefth_q4 + hefth_q5 + hefth_q6 + hefth_q7 + hefth_q8 + hefth_q9 + hefth_q10) / 2);
                } else if (no_of_questions == '5') {
                    hefth = (hefth_q1 + hefth_q2 + hefth_q3 + hefth_q4 + hefth_q5);
                } else {
                    hefth = "";
                }
                //----------------------------------------------------------------------------------------------------------------------------------

                Total_Mark = ((telawa * 0.25) + (hefth * 0.75));

                //----------------------------------------------------------------------------------------------------------------------------------
				
				if (Total_Mark >= 90) {
				document.getElementById('Grade').value  = ("ممتاز");
					Grade = ("ممتاز");
				} else if (Total_Mark >= 80) {
					document.getElementById('Grade').value = ("جيدجداً");
				Grade = ("جيدجداً");
				} else if (Total_Mark >= 70) {
					document.getElementById('Grade').value = ("جيد");
				Grade = ("جيد");
				} else {
					document.getElementById('Grade').value = ("راسب");
				Grade = ("راسب");
				}
                //----------------------------------------------------------------------------------------------------------------------------------

				console.log("Grade" +Grade);
				




                //----------------------------------------------------------------------------------------------------------------------------------
                document.getElementById('tajweed_final').innerHTML = tajweed_final;
                document.getElementById('daqaeq_final').innerHTML = daqaeq_final;
                document.getElementById('salasa_final').innerHTML = salasa_final;
                document.getElementById('tazyeen_final').innerHTML = tazyeen_final;
                document.getElementById('waqf_final').innerHTML = waqf_final;
                document.getElementById('hefth_q1').innerHTML = hefth_q1;
                document.getElementById('hefth_q2').innerHTML = hefth_q2;
                document.getElementById('hefth_q3').innerHTML = hefth_q3;
                document.getElementById('hefth_q4').innerHTML = hefth_q4;
                document.getElementById('hefth_q5').innerHTML = hefth_q5;
                document.getElementById('hefth_q6').innerHTML = hefth_q6;
                document.getElementById('hefth_q7').innerHTML = hefth_q7;
                document.getElementById('hefth_q8').innerHTML = hefth_q8;
                document.getElementById('hefth_q9').innerHTML = hefth_q9;
                document.getElementById('hefth_q10').innerHTML = hefth_q10;
                document.getElementById('no_of_questions').innerHTML = no_of_questions;
                document.getElementById('max_value').innerHTML = max_value;
                document.getElementById('zero_value').innerHTML = zero_value;
                document.getElementById('telawa').innerHTML = telawa;
                document.getElementById('hefth').innerHTML = hefth;
                document.getElementById('Total_Mark').innerHTML = Total_Mark;
				document.getElementById('Grade').innerHTML = Grade;
				
                //----------------------------------------------------------------------------------------------------------------------------------


                console.log("Telwa" + telawa);

            }
        </script>


